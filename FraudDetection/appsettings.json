{
  "ConnectionStrings": {
    "Redis": "localhost:6379",
    "RabbitMQ": "amqp://guest:guest@localhost:5672/"
  },
  "FraudDetection": {
    "RealTimeScoring": {
      "Enabled": true,
      "LatencyThresholdMs": 100,
      "CacheExpirationMinutes": 30
    },
    "ML": {
      "ModelPath": "./Models/fraud_detection_model.zip",
      "RetrainFrequencyHours": 24,
      "FeatureImportanceThreshold": 0.1
    },
    "RiskScoring": {
      "HighRiskThreshold": 0.8,
      "MediumRiskThreshold": 0.5,
      "LowRiskThreshold": 0.2
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
